{
  "schemaVersion": 1,
  "id": "easy_item_list",
  "version": "${version}",
  "name": "EasyItemList",
  "description": "Automatically adds server side custom items to JEI, REI, and EMI!",
  "authors": [ "JustAHuman_xD" ],
  "contact": {
    "homepage": "https://modrinth.com/mod/easy-item-list",
    "sources": "https://github.com/JustAHuman-xD/EasyItemList",
    "issues": "https://github.com/JustAHuman-xD/EasyItemList/issues",
    "discord": "https://discord.gg/y6fV6pbgHB"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/y6fV6pbgHB"
      },
      "update_checker": true
    }
  },
  "license": "MIT",
  "icon": "assets/easy_item_list/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.justahuman.easy_item_list.EasyItemList"
    ],
    "emi": [
      "me.justahuman.easy_item_list.hooks.EmiHook"
    ],
    "jei_mod_plugin": [
      "me.justahuman.easy_item_list.hooks.JeiHook"
    ],
    "rei_client": [
      "me.justahuman.easy_item_list.hooks.ReiHook"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=${minecraft_version}"
  }
}