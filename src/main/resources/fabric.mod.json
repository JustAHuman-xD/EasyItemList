{
  "schemaVersion": 1,
  "id": "easyitemlist",
  "version": "${version}",
  "name": "EasyItemList",
  "description": "Automatically adds nbt items in serverside recipes to EMI!",
  "authors": [
    "JustAHuman"
  ],
  "contact": {
    "website": "https://github.com/JustAHuman-xD/EasyItemList",
    "repo": "https://github.com/JustAHuman-xD/EasyItemList"
  },
  "license": "GPL-3.0",
  "icon": "assets/easyitemlist/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.justahuman.easyitemlist.EasyItemList"
    ],
    "emi": [
      "me.justahuman.easyitemlist.hooks.EmiHook"
    ],
    "jei_mod_plugin": [
      "me.justahuman.easyitemlist.hooks.JeiHook"
    ],
    "rei_client": [
      "me.justahuman.easyitemlist.hooks.ReiHook"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": ">=${minecraft_version}"
  }
}
